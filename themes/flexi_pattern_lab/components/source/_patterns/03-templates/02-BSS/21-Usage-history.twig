{% extends 'organisms-dashboard-container' %}

{% block dashboardContent %}
  <div class="py-50 py-md-120 bg-lightest-gray">
    <div class="container">
      <div class="row justify-content-center">

         <div class="col-12 text-center mb-4">
          <h1>Data Usage</h1>
          <p class="text-muted">514-428-0578</p>
        </div>

         <div class="col-12 mb-5 text-center">
          {% include 'molecule-time-navigation' %}
        </div>

          {% include 'molecule-bar-graph' %}

         <div class="col-12 mb-5 col-lg-4 mt-5 text-center">
          {% include 'atoms-button' with{ buttonLabel:'Download Usage History',buttonSize:'l', classes:'btn-block'} %}
        </div>
      </div>
    </div>
  </div>

{% endblock %}